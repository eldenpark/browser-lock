{
  "background": {
    "page": "html/background.html"
  },
  "content_scripts": [
    {
      "js": [
        "js/content.js"
      ],
      "matches": [
        "\u003Call_urls\u003E"
      ]
    }],
  "browser_action" : {
      "default_popup": "html/popup.html"
   },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "default_locale": "en",
  "description": "__MSG_descriptionjson__",
  "icons": {
    "128": "img/logo-128.png",
    "48": "img/logo-48.png",
    "16": "img/logo-16.png"
  },
  "manifest_version": 2,
  "minimum_chrome_version": "22",
  "name": "Pattern Lock Extension",
  "options_page": "html/options.html",
  "chrome_url_overrides" : {
    "newtab": "html/lock.html"
  },

  "permissions": [
    "cookies",
    "tabs",
    "\u003Call_urls>",
    "clipboardRead",
    "clipboardWrite",
    "bookmarks",
    "notifications",
    "storage",
    "unlimitedStorage",
    "webNavigation",
    "webRequest",
    "webRequestBlocking",
    "cookies",
    "browsingData",
    "history"
  ],
  "version": "2.9.2",
  "web_accessible_resources": [
    "img/*",
    "js/*"
  ]
}
